{
  "name": "@yearn/yearn-web-tokens",
  "version": "0.0.3",
  "description": "Logo of tokens use in web ecosystem as react components",
  "author": "Yearn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yearn/yearn-assets.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prebuild": "python3 ./scripts/prepare.py",
    "build": "webpack --mode production",
    "start": "webpack --mode development",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prepublish": "python3 ./scripts/prepare.py && webpack --mode production"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
